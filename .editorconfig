root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
tab_width = 4
trim_trailing_whitespace = true

[*.gradle]
indent_style = tab

[*.gradle.kts]
indent_style = tab
indent_size = 4

[*.java]
indent_style = tab

[*.json]
indent_style = tab
indent_size = 4

[*.toml]
indent_style = tab

[*.properties]
indent_style = tab
indent_size = 4

[.editorconfig]
indent_style = tab
indent_size = 4
